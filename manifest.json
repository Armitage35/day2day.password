{
	"short_name": "Day2Day.Password",
	"name": "Day2Day.Password",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "favicon.ico",
		"default_popup": "index.html"
	},
	"permissions": ["clipboardWrite"],
	"icons": {
		"16": "favicon.ico",
		"48": "favicon.ico",
		"128": "favicon.ico"
	},
	"version": "1.3",
	"start_url": "./index.html",
	"display": "standalone",
	"theme_color": "#000000",
	"background_color": "#ffffff",
	"content_security_policy": "script-src 'self' 'unsafe-eval'; https://*.segment.com https://*.mxpnl.com; object-src 'self'"
}
